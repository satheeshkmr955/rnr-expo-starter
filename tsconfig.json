{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["*"]
    },
    "plugins": [
      {
        "name": "@0no-co/graphqlsp",
        "schema": "https://graphqlplaceholder.vercel.app/graphql/"
      }
    ]
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "nativewind-env.d.ts"]
}
